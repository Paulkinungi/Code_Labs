{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@codelabs_api_url",
    "REACT_APP_SOCKET_URL": "@codelabs_socket_url"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "@codelabs_api_url",
      "REACT_APP_SOCKET_URL": "@codelabs_socket_url"
    }
  }
}
